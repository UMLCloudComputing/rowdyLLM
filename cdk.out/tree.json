{
  "version": "tree-0.1",
  "tree": {
    "id": "App",
    "path": "",
    "children": {
      "CdkStack": {
        "id": "CdkStack",
        "path": "CdkStack",
        "children": {
          "AmazonBedrockAgentRole": {
            "id": "AmazonBedrockAgentRole",
            "path": "CdkStack/AmazonBedrockAgentRole",
            "attributes": {
              "aws:cdk:cloudformation:type": "AWS::IAM::Role",
              "aws:cdk:cloudformation:props": {
                "assumeRolePolicyDocument": {
                  "Version": "2012-10-17",
                  "Statement": [
                    {
                      "Effect": "Allow",
                      "Principal": {
                        "Service": "bedrock.amazonaws.com"
                      },
                      "Action": "sts:AssumeRole"
                    }
                  ]
                },
                "policies": [
                  {
                    "policyName": "AmazonBedrockAgentPolicy",
                    "policyDocument": {
                      "Version": "2012-10-17",
                      "Statement": [
                        {
                          "Effect": "Allow",
                          "Action": "bedrock:InvokeModel",
                          "Resource": "*"
                        },
                        {
                          "Effect": "Allow",
                          "Action": "bedrock:Retrieve",
                          "Resource": "*"
                        }
                      ]
                    }
                  }
                ],
                "roleName": "AmazonBedrockAgentRole_CdkStack"
              }
            },
            "constructInfo": {
              "fqn": "aws-cdk-lib.aws_iam.CfnRole",
              "version": "2.144.0"
            }
          },
          "Agent": {
            "id": "Agent",
            "path": "CdkStack/Agent",
            "attributes": {
              "aws:cdk:cloudformation:type": "AWS::Bedrock::Agent",
              "aws:cdk:cloudformation:props": {
                "agentName": "MyAgentCdkStack",
                "agentResourceRoleArn": {
                  "Fn::GetAtt": [
                    "AmazonBedrockAgentRole",
                    "Arn"
                  ]
                },
                "autoPrepare": true,
                "description": "Production",
                "foundationModel": "amazon.titan-text-premier-v1:0",
                "instruction": "Your identity is Rowdy the Riverhawk. Your job is the answer questions about the University of Massachusetts Lowell"
              }
            },
            "constructInfo": {
              "fqn": "aws-cdk-lib.aws_bedrock.CfnAgent",
              "version": "2.144.0"
            }
          },
          "CDKMetadata": {
            "id": "CDKMetadata",
            "path": "CdkStack/CDKMetadata",
            "children": {
              "Default": {
                "id": "Default",
                "path": "CdkStack/CDKMetadata/Default",
                "constructInfo": {
                  "fqn": "aws-cdk-lib.CfnResource",
                  "version": "2.144.0"
                }
              },
              "Condition": {
                "id": "Condition",
                "path": "CdkStack/CDKMetadata/Condition",
                "constructInfo": {
                  "fqn": "aws-cdk-lib.CfnCondition",
                  "version": "2.144.0"
                }
              }
            },
            "constructInfo": {
              "fqn": "constructs.Construct",
              "version": "10.3.0"
            }
          },
          "BootstrapVersion": {
            "id": "BootstrapVersion",
            "path": "CdkStack/BootstrapVersion",
            "constructInfo": {
              "fqn": "aws-cdk-lib.CfnParameter",
              "version": "2.144.0"
            }
          },
          "CheckBootstrapVersion": {
            "id": "CheckBootstrapVersion",
            "path": "CdkStack/CheckBootstrapVersion",
            "constructInfo": {
              "fqn": "aws-cdk-lib.CfnRule",
              "version": "2.144.0"
            }
          }
        },
        "constructInfo": {
          "fqn": "aws-cdk-lib.Stack",
          "version": "2.144.0"
        }
      },
      "Tree": {
        "id": "Tree",
